{
    "extends": "../tslint.json",
    "rules": {
      "semicolon": [
          true,
          "never"
        ],
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ]
    }
}
